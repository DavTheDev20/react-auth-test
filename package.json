{
  "name": "react-auth-test",
  "version": "1.0.0",
  "description": "This repo is a full stack application using node for the backend and react for the frontend. It is meant to test how to implement auth into a react frontend.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./server/out/server.js",
    "server": "nodemon ./server/out/server.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently -n \"server,client\" -c \"red,blue\" \"npm run server\" \"npm run client\""
  },
